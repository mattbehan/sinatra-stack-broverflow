      <label for="email">Email (required, but never shown)</label>
      <input type="text" name="user[email]">
      <br>

      <label for="password">Password</label>
      <input type="password" name="user[password]">
      <br>

      <label for="confirm_password">Confirm Password</label>
      <input type="confirm_password" name="user[confirm_password]">
      <br>

      <a id="signup-options" href="/">more login options</a>

      <input type="submit" name="register" value="Sign up">
      <br>

      <p id="terms">By registering, you agree to the <a id="signup-options" href="/">privacy policy</a> and <a id="signup-options" href="/">terms of service</a>.
    </form>
  </div>

