<body>
  <div class="topbar">
    <div class="topbar-wrapper">
      <div class="topbar-links">
        <div class="links-container">
          <span class="topbar-menu-links">
            <% if current_user %>
            <a href="/logout">logout</a>
            <% else %>
            <a href="/users/new">sign up</a>
            <a href="/login">login</a>
            <% end %>
          </span>
        </div>
        <div class="search-container">
          <form id="search" action="/search" method="GET" autocomplete="off">
            <input name="q" type="text" placeholder="search" value tabindex="1" autocomplete="off" maxlength="240" />
          </form>
        </div>
      </div>
    </div>
  </div>
  <div class="container">
  <header>
    <br class="cbt">
    <div id="hlogo">
      <a href="/">Stack Overflow</a>
    </div>
    <div id="hmenus">
      <div class="mainnavs">
        <ul>
          <li><a id="nav-questions" href="/questions">Questions</a></li>
          <li><a id="nav-tags" href="/tags">Tags</a></li>
          <li><a id="nav-users" href="/users">Users</a></li>
          <li><a id="nav-badges" href="/help/badges">Badges</a></li>
          <li><a id="nav-unanswered" href="/unanswered">Unanswered</a></li>
          <li><a id="nav-askquestion" href="/questions/new">Ask Question</a></li>
        </ul>
      </div>
    </div>
  </header>
  <div id="content">
    <div id="mainbar">
      <div class="subheader">
        <h1>Top Questions</h1>
        <div id="tabs">
          <a class="youarehere" href="">interesting</a>
          <a href="?tab=featured">featured</a>
          <a href="?tab=hot">hot</a>
          <a href="?tab=week">week</a>
          <a href="?tab=month">month</a>
        </div>
      </div>
      <div id="question-wrapper">
        <%= yield %>
      </div>
    </div>
    <div id="sidebar">
      <div id="jobs-ad"><a href="" class="top">Bro Work Near You</a>
        <ul class="jobs">
          <li>
            <a href="http://careers.stackoverflow.com/jobs/94921/sr-software-engineer-in-test-iloan-a-springleaf-brand">Assistant Lax Coach</a><br><div class="company">Chad University</div><div class="location">Long Island, NY</div><span class="tags">ruby-on-rails</span>
          </li>
          <li>
            <a href="http://careers.stackoverflow.com/jobs/94921/sr-software-engineer-in-test-iloan-a-springleaf-brand">Financial Advisor</a><br><div class="company">iLoan, a Springleaf brand</div><div class="location">Chicago, IL</div><span class="tags">ruby-on-rails</span>
          </li>
          <a href="http://careers.stackoverflow.com/jobs/location/Chicago%2C%20Illinois%2C%20United%20States?utm_source=stackoverflow.com&utm_medium=ad&utm_campaign=small-sidebar-blue-nearyou">More jobs in Chicago</a>
        </ul>
      </div>
      <div id="hot-network-questions" class="module">
        <h4>Hot Network Questions</h4>
        <ul>
          <li>
            <a href="http://puzzling.stackexchange.com/questions/22243/art-thou-addressing-me">Art Thou Addressing Me?</a>
          </li>
        </ul>
      </div>
    </div>
  </div>
</div>
</body>

